{
    "name": "kaisarcode/psmod",
    "description": "Generic PrestaShop module.",
    "authors": [
        {
            "name": "KaisarCode",
            "email": "kaisar@kaisarcode.com"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4": {
            "KaisarCode\\PsMod\\": "src/"
        },
        "classmap": [
            "classes/PsModExampleModelClass.php"
        ],
        "exclude-from-classmap": []
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false
    },
    "type": "prestashop-module",
    "license": "Academic Free License (AFL 3.0)"
}